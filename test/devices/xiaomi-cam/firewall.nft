#!/usr/sbin/nft -f

table xiaomi-cam {

    chain dns {
        type filter hook prerouting priority 0;
        
        # Match HTTP requests (TCP * -> 80)
        tcp sport 1111 tcp dport 2222 queue num 3;

        # Match HTTP responses (TCP 80 -> *)
        tcp sport 1111 tcp dport 2222 queue num 3;
    }

}
